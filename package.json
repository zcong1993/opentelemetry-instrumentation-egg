{
  "name": "opentelemetry-instrumentation-egg",
  "version": "0.3.4",
  "description": "OpenTelemetry Egg automatic instrumentation package.",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run lint && npm run jest",
    "test:cov": "npm run lint && npm run jest:cov",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write '**/*.{ts,json,md}'",
    "jest:cov": "jest --coverage",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "version": "node ./scripts/update-version.js && git add .",
    "release": "js-release"
  },
  "types": "./dist/index.d.ts",
  "author": "zcong1993",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@opentelemetry/instrumentation-http": "^0.28.0",
    "@opentelemetry/resources": "^1.0.0",
    "@opentelemetry/sdk-node": "^0.28.0",
    "@opentelemetry/sdk-trace-base": "^1.0.0",
    "@opentelemetry/sdk-trace-node": "^1.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@zcong/js-release": "^0.2.5",
    "egg-core": "^4.20.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^26.0.0",
    "husky": "^7.0.2",
    "jest": "^28.0.0",
    "lint-staged": "^12.0.0",
    "prettier": "^2.4.1",
    "ts-jest": "^28.0.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.0.3",
    "@opentelemetry/core": "^1.0.0",
    "@opentelemetry/instrumentation": "^0.28.0",
    "@opentelemetry/semantic-conventions": "^1.0.0",
    "@types/koa": "2.13.4",
    "@types/koa__router": "8.0.11"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "lint-staged": {
    "*.{js,json,ts,md}": "prettier --write"
  },
  "release": {
    "branches": [
      "release"
    ]
  }
}
